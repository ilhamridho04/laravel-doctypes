{
    "name": "ngodingskuyy/doctypes",
    "description": "Dynamic DocType system for Laravel - Create Frappe-like dynamic document types with Vue + Tailwind CSS",
    "type": "library",
    "keywords": [
        "laravel",
        "doctype",
        "dynamic-forms",
        "frappe",
        "vue",
        "tailwind",
        "json-schema",
        "form-builder"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilham Ridho Asysyifa'a",
            "email": "ilhamridho.ir@gmail.com"
        }
    ],
    "homepage": "https://github.com/ilhamridho04/laravel-doctypes",
    "support": {
        "issues": "https://github.com/ilhamridho04/laravel-doctypes/issues",
        "source": "https://github.com/ilhamridho04/laravel-doctypes"
    },
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/validation": "^11.0|^12.0",
        "spatie/laravel-json-api-paginate": "^1.16",
        "spatie/laravel-query-builder": "^6.3"
    },
    "suggest": {
        "laravel/framework": "^11.0|^12.0",
        "inertiajs/inertia-laravel": "^1.0",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.0|^10.0",
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Ngodingskuyy\\Doctypes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ngodingskuyy\\Doctypes\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Ngodingskuyy\\Doctypes\\Providers\\DoctypeServiceProvider"
            ],
            "aliases": {
                "Doctype": "Ngodingskuyy\\Doctypes\\Facades\\Doctype"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}