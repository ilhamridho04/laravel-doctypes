{
    "name": "ngodingskuyy/doctypes",
    "description": "Dynamic DocType system for Laravel - Create Frappe-like dynamic document types",
    "type": "library",
    "keywords": [
        "laravel",
        "doctype",
        "dynamic-forms",
        "frappe",
        "vue"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "NgodingSkuy",
            "email": "support@ngodingskuy.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/validation": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Doctypes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctypes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Doctypes\\Providers\\DoctypeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}